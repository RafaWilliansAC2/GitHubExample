{
	"name": "Codespace Main",
	"image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu",
	"postCreateCommand": "sudo apt-get update && sudo apt-get install -y gh",
	"customizations": {
		"vscode": {
			"extensions": [
				"mhutchie.git-graph",
				"phil294.git-log--graph"
			]
		}
}
}
